<Window x:Class="WpfApplication1.GetSharpointInfo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Icon="/WpfApplication1;component/Images/IceCubeLogo.png"
        Title="GetSharpointInfo" Height="300" Width="300" >
    
    <Grid  Background="{StaticResource Background1}" >
        <Button Content="Show Document" Height="23" HorizontalAlignment="Left" Margin="79,218,0,0" Name="btnGetInfo" VerticalAlignment="Top" Width="100" Click="btnGetInfo_Click" />
        <GroupBox Header="Sharepoint Documents" Height="200" HorizontalAlignment="Left" Margin="12,12,0,0" Name="groupBox1" VerticalAlignment="Top" Width="254">
            <TextBox Height="178" HorizontalAlignment="Left" Name="txtInfo" VerticalAlignment="Top" Width="243" />
          
        </GroupBox>
        <Button Content="Close" Height="23" HorizontalAlignment="Left" Margin="203,218,0,0" Name="btnClose" VerticalAlignment="Top" Width="63" Click="btnClose_Click" />
    </Grid>
</Window>
