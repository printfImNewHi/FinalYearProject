<Window x:Class="WpfApplication1.PreviewImage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="PreviewImage" Height="300" Width="300" Icon="/WpfApplication1;component/Images/IceCubeLogo.png">
    <Grid Background="{StaticResource Background1}">
        <Image Height="34" HorizontalAlignment="Left" Name="image1" Stretch="Fill" VerticalAlignment="Top" Width="106" Source="/WpfApplication1;component/Images/IceCubeLogo.png" Margin="12,12,0,0" Grid.RowSpan="2" />
        <GroupBox Header="List of Documents to be Uploaded" Height="163" HorizontalAlignment="Left" Margin="12,52,0,0" Name="groupBox1" VerticalAlignment="Top" Width="254">
            <ListView Height="148" Name="listView1" Width="242" />
        </GroupBox>
        <Grid Margin="0" Grid.Row="2">
            <Button Content="Close" Height="23" HorizontalAlignment="Left" Margin="185,223,0,0" Name="button1" VerticalAlignment="Top" Width="75" Click="close_Click" />
        </Grid>
    </Grid>
</Window>
